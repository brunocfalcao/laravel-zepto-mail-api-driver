{
    "name": "brunocfalcao/laravel-zepto-mail-api-driver",
    "description": "Zeptomail api mail driver for Laravel",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/brunocfalcao/laravel-zepto-mail-api-driver",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Brunocfalcao\\ZeptoMailApiDriver\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brunocfalcao\\ZeptoMailApiDriver\\ZeptoMailApiDriverServiceProvider"
            ]
        }
    }
}
